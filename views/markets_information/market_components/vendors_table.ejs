<table class="table table-hover table-striped">
    <thead>
        <tr>
            <th>名稱</th>
            <th>類型</th>
            <th>地址</th>
            <th>資訊</th>
        </tr>
    </thead>
    <tbody>
    <% items.forEach(function (data) {%>
        <tr>
            <form action="/areas/vendors" method="get">
                <td><h4><input type="hidden" name="vendorName" value=<%=data.vendor%>><%=data.vendor%></h4></td>
                <td><h4><%=data.select_category%></h4></td>
                <td><h4><%=data.address%></h4></td>
                <td><input type="submit" class="btn btn-info" value="前往"></td>
            </form>
        </tr>
    <%}) %>
    </tbody>
</table>
